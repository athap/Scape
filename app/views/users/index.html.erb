<div class="logo-center">
  <%#= link_to image_tag("logo.png", alt: "Rails"), 'http://idias.usu.edu/scape' %>
</div>

<div class="center hero-unit">
  <h1>Scape Data Collection</h1>
</div>

<div class="users-table table table-striped">
  <table>
    <thead>
      <tr>
        <th>Humans</th>
      </tr>
    </thead>
    <tbody>
      <%= render @users %>      
    </tbody> 
  </table>  
</div>


<div class="quizzes-table table table-striped">
  <table>
    <thead>
      <tr>
        <th>Question</th>
        <th>Selected Answer</th>
        <th>Correct Answer</th>
        <th>Points</th>
      </tr>
    </thead>

    <tbody id="quizzes">
      <%= render :partial => 'user_exams', :collection => @exams, :as => :exam %>
    </tbody>  
  </table>
</div>